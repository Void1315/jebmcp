{
    "mcpServers": {
      "jeb": {
        "command": "cmd",
        "args": [
          "/c",
          "uv",
          "--directory",
          "jebmcp\\jeb-mcp\\src\\jeb_mcp",
          "run",
          "server.py"
        ],
        "timeout": 1800,
        "disabled": false,
        "autoApprove": [
          "get_manifest",
          "get_method_callers",
          "get_class_decompiled_code",
          "get_method_decompiled_code",
          "check_connection",
          "ping",
          "get_method_overrides"
        ]
      }
    }
}